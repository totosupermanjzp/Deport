@model Depot.Models.Person

@{
    ViewData["Title"] = "Delete";
}

<h2>删除</h2>

<h3>您真的要删除它吗？？</h3>
<div>
    <h4>Person</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            部门编号
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DepartmentId)
        </dd>
        <dt>
            部门名
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PersonManage)
        </dd>
        <dt>
            名字
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PersonName)
        </dd>
        <dt>
            权限
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Power)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            <a asp-action="Index">Back to List</a>
        </div>
    </form>
</div>
